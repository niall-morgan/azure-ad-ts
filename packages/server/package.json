    "@tsed/di": "^7.34.5",
    "@tsed/engines": "^7.34.5",
    "@tsed/exceptions": "^7.34.5",
    "@tsed/json-mapper": "^7.34.5",
    "@tsed/logger": "^6.6.2",
    "@tsed/logger-file": "^6.6.2",
    "@tsed/openspec": "^7.34.5",
    "@tsed/passport": "^7.34.5",
    "@tsed/platform-cache": "^7.34.5",
    "@tsed/platform-exceptions": "^7.34.5",
    "@tsed/platform-express": "^7.34.5",
    "@tsed/platform-log-middleware": "^7.34.5",
    "@tsed/platform-middlewares": "^7.34.5",
    "@tsed/platform-params": "^7.34.5",
