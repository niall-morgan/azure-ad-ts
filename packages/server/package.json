    "@tsed/schema": "^7.34.5",
    "@tsed/swagger": "^7.34.5",
    "ajv": "^8.12.0",
    "barrelsby": "^2.8.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "passport": "^0.6.0",
    "passport-azure-ad": "4.3.5"
