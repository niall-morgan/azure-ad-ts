    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/passport": "^1.0.12",
    "@types/supertest": "^2.0.12",
    "@types/passport-azure-ad": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
